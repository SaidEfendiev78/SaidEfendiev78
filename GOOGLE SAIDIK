print ("Привет! мы словать по типу гугла!")
meme_dict = {
            "КРИНЖ": "Что-то очень странное или стыдное",
            "ЛОЛ": "Что-то очень смешное",
            "ПАРДОН": "Извинения на французском",
            "РОФЛ": "шутка",
            "АГРИТСЯ": "Злобный, злой",
            "КРИПОВЫЙ": "Страшный, пугающий",
            "ЧЕЛ": "человек, хомосапиенс"
            }
while True:
    word = input("Введите непонятное слово (большими буквами!): ")
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print("такого слово не существует, это матрица чел. это сон")
    
